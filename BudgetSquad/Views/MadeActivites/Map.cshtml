
@model BudgetingSquad.Models.GeocodeInfo
@{
    ViewBag.Title = "Map";
}

<h2>Map</h2>
<style>
    /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
    #map {
        height: 400px;
        width: 400px;
    }
</style>

<div class="row">
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            var dcc = {lat: 43.034271, lng:-87.911919}
            //var location = { lat: @*@Html.DisplayFor(model => model.Latitude), lng: @Html.DisplayFor(model => model.Address.Longitude) };*@
            
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 43.0389, lng: -87.9065 },
                zoom: 8
            });
            var marker = new google.maps.Marker({ position: dcc, map: map });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCgxbc-4DbwDz3-fH-pSijZdrbh6JL-i4E&callback=initMap"
            async defer></script>
</div>

